{
"branches": ["v5.x"],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        ["@semantic-release/exec", {
		"shell": "python setup.py bdist_wheel --universal"
	}],
        ["@semantic-release/exec", {
                "prepareCmd": 'sed -ir "s/[0-9].[0-9].[0-9]/${nextRelease.version}/" bench/__init__.py'
        }],
	"@semantic-release/git",
	["@semantic-release/github", {
      		"assets": [
        		{"path": "dist/frappe_bench-${nextRelease.version[1:]}-py2.py3-none-any.whl", "label": "frappe_bench-${nextRelease.version[1:]}-py2.py3-none-any.whl"},
      		]
	}]
    ]
}
